<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:epmem="http://www.dfki.de/lt/onto/pal/episodicmemory.owl#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xml:base="http://www.dfki.de/lt/onto/pal/episodicmemory.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://www.dfki.de/lt/onto/pal/domain.owl"/>
    <owl:imports rdf:resource="http://www.dfki.de/lt/onto/common/upper.owl"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >author(s): Bart Schreuder Goedheijt</rdfs:comment>
  </owl:Ontology>
  <owl:Class rdf:ID="GoalEvent">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Event"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="EpisodeTag"/>
  <owl:Class rdf:ID="Episode">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Implementation of an affective-based episodic memory</rdfs:comment>
    <owl:disjointWith rdf:resource="http://www.dfki.de/lt/onto/pal/domain.owl#PALDomain"/>
    <owl:disjointWith rdf:resource="http://www.dfki.de/lt/onto/common/upper.owl#Entity"/>
  </owl:Class>
  <owl:Class rdf:about="#Event">
    <owl:equivalentClass>
      <rdf:Description rdf:about="http://www.dfki.de/lt/onto/common/upper.owl#Event">
        <owl:equivalentClass rdf:resource="#Event"/>
      </rdf:Description>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="DiaryEvent">
    <rdfs:subClassOf rdf:resource="#Event"/>
  </owl:Class>
  <owl:Class rdf:ID="EpisodeTrigger"/>
  <owl:Class rdf:ID="TimelineEvent">
    <rdfs:subClassOf rdf:resource="#Event"/>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="hasEventType"/>
  <owl:ObjectProperty rdf:ID="hasTags">
    <rdfs:range rdf:resource="#EpisodeTag"/>
    <rdfs:domain rdf:resource="#Episode"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="isTriggeredBy">
    <rdfs:domain rdf:resource="#EpisodeTag"/>
    <rdfs:range rdf:resource="#EpisodeTrigger"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasAffection">
    <rdfs:range rdf:resource="http://www.dfki.de/lt/onto/pal/domain.owl#Mood"/>
    <rdfs:domain rdf:resource="#Episode"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >&lt;p style="margin-top: 0"&gt;
      This should point to the yet to be developed affection ontology (Anne 
      Merel Sternheim is working on this)
    &lt;/p&gt;</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="containsEvent">
    <rdfs:range rdf:resource="#Event"/>
    <rdfs:domain rdf:resource="#Episode"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="sharedWith">
    <rdfs:range rdf:resource="http://www.dfki.de/lt/onto/pal/domain.owl#Child"/>
    <rdfs:domain rdf:resource="#Episode"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >&lt;p style="margin-top: 0"&gt;
      Episodic memory can be shared with a child
    &lt;/p&gt;</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasBeenTold">
    <rdfs:range rdf:resource="http://www.dfki.de/lt/onto/common/upper.owl#Situation"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="ObjectProperty_17"/>
  <owl:DatatypeProperty rdf:ID="E"/>
  <owl:DatatypeProperty rdf:ID="DatatypeProperty_15"/>
  <owl:DatatypeProperty rdf:ID="lastTimeTold">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#long"/>
    <rdfs:domain rdf:resource="#Episode"/>
  </owl:DatatypeProperty>
  <epmem:EpisodeTag rdf:ID="GoalCompleted">
    <epmem:isTriggeredBy>
      <epmem:EpisodeTrigger rdf:ID="OnChildLogin"/>
    </epmem:isTriggeredBy>
  </epmem:EpisodeTag>
  <epmem:EpisodeTag rdf:ID="GoalsUpdated">
    <epmem:isTriggeredBy rdf:resource="#OnChildLogin"/>
  </epmem:EpisodeTag>
  <epmem:EpisodeTag rdf:ID="DiaryEntriesSummary">
    <epmem:isTriggeredBy rdf:resource="#OnChildLogin"/>
  </epmem:EpisodeTag>
  <owl:AllDifferent/>
  <epmem:EpisodeTrigger rdf:ID="OnQuizStart"/>
  <epmem:EpisodeTrigger rdf:ID="OnDiaryClose"/>
  <epmem:EpisodeTrigger rdf:ID="OnTimelineClose"/>
  <epmem:EpisodeTrigger rdf:ID="OnTimelineOpen"/>
  <epmem:EpisodeTag rdf:ID="GoalWasTooDifficult">
    <epmem:isTriggeredBy rdf:resource="#OnChildLogin"/>
  </epmem:EpisodeTag>
  <epmem:EpisodeTrigger rdf:ID="OnQuizEnd"/>
  <epmem:EpisodeTag rdf:ID="GoalAlmostAchieved">
    <epmem:isTriggeredBy rdf:resource="#OnChildLogin"/>
  </epmem:EpisodeTag>
  <epmem:EpisodeTag rdf:ID="DiaryHobbyPlayed">
    <epmem:isTriggeredBy>
      <epmem:EpisodeTrigger rdf:ID="OnDiaryOpen"/>
    </epmem:isTriggeredBy>
  </epmem:EpisodeTag>
  <epmem:EpisodeTag rdf:ID="MeasurementSummary">
    <epmem:isTriggeredBy rdf:resource="#OnChildLogin"/>
  </epmem:EpisodeTag>
</rdf:RDF>

