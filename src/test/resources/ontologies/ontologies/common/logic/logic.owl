<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:logic="http://www.dfki.de/lt/onto/common/logic.owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://www.dfki.de/lt/onto/common/logic.owl">
  <owl:Ontology rdf:about="">
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >v0.9.1</owl:versionInfo>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >author(s): Hans-Ulrich Krieger</rdfs:comment>
  </owl:Ontology>
  <owl:Class rdf:ID="Unlikely">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="DontKnow"/>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >unlikely</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#DontKnow">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >complete uncertainty</rdfs:label>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >don't know</rdfs:label>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >unknown</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="NotExcluded">
    <rdfs:subClassOf rdf:resource="#DontKnow"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >not excluded</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="Excluded">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >excluded</rdfs:label>
    <rdfs:subClassOf rdf:resource="#Unlikely"/>
  </owl:Class>
  <owl:Class rdf:ID="Confirmed">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >confirmed</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Likely"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Perhaps">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >perhaps</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="FiftyFifty"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#FiftyFifty">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >fifty-fifty</rdfs:label>
    <rdfs:subClassOf rdf:resource="#DontKnow"/>
  </owl:Class>
  <owl:Class rdf:ID="False">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >bottom</rdfs:label>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >false</rdfs:label>
    <rdfs:subClassOf rdf:resource="#Excluded"/>
  </owl:Class>
  <owl:Class rdf:about="#Likely">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >likely</rdfs:label>
    <rdfs:subClassOf rdf:resource="#NotExcluded"/>
  </owl:Class>
  <owl:Class rdf:ID="True">
    <rdfs:subClassOf rdf:resource="#Confirmed"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >top</rdfs:label>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >true</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="Error">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >error</rdfs:label>
    <rdfs:subClassOf rdf:resource="#False"/>
    <rdfs:subClassOf rdf:resource="#True"/>
    <rdfs:subClassOf rdf:resource="#Perhaps"/>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="PerhapsNot"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#PerhapsNot">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >perhaps not</rdfs:label>
    <rdfs:subClassOf rdf:resource="#FiftyFifty"/>
  </owl:Class>
  <owl:Class rdf:ID="Proposition">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >&lt;p style="margin-top: 0"&gt;
      a reified version of the quintuple &amp;lt;polarity, subject, predicate, 
      object, transactiontime&amp;gt; that can be easily interfaced with 
      Event/Situation classes, if needed
    &lt;/p&gt;</rdfs:comment>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="polarity">
    <rdfs:domain rdf:resource="#Proposition"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="#DontKnow"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="predicate">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
    <rdfs:domain rdf:resource="#Proposition"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="object">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain rdf:resource="#Proposition"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="ttime">
    <rdfs:domain rdf:resource="#Proposition"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#long"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="subject">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain rdf:resource="#Proposition"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
  </owl:DatatypeProperty>
  <owl:FunctionalProperty rdf:ID="low">
    <rdfs:domain rdf:resource="#DontKnow"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="high">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
    <rdfs:domain rdf:resource="#DontKnow"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <logic:PerhapsNot rdf:ID="perhapsnot">
    <logic:high rdf:datatype="http://www.w3.org/2001/XMLSchema#double"
    >0.5</logic:high>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >always use this singleton instance</rdfs:comment>
    <logic:low rdf:datatype="http://www.w3.org/2001/XMLSchema#double"
    >0.4</logic:low>
  </logic:PerhapsNot>
  <logic:Likely rdf:ID="likely">
    <logic:high rdf:datatype="http://www.w3.org/2001/XMLSchema#double"
    >1.0</logic:high>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >always use this singleton instance</rdfs:comment>
    <logic:low rdf:datatype="http://www.w3.org/2001/XMLSchema#double"
    >0.7</logic:low>
  </logic:Likely>
  <logic:False rdf:ID="false">
    <logic:high rdf:datatype="http://www.w3.org/2001/XMLSchema#double"
    >0.0</logic:high>
    <logic:low rdf:datatype="http://www.w3.org/2001/XMLSchema#double"
    >0.0</logic:low>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >always use this singleton instance</rdfs:comment>
  </logic:False>
  <logic:True rdf:ID="true">
    <logic:low rdf:datatype="http://www.w3.org/2001/XMLSchema#double"
    >1.0</logic:low>
    <logic:high rdf:datatype="http://www.w3.org/2001/XMLSchema#double"
    >1.0</logic:high>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >always use this singleton instance</rdfs:comment>
  </logic:True>
  <logic:FiftyFifty rdf:ID="fiftyfifty">
    <logic:high rdf:datatype="http://www.w3.org/2001/XMLSchema#double"
    >0.6</logic:high>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >always use this singleton instance</rdfs:comment>
    <logic:low rdf:datatype="http://www.w3.org/2001/XMLSchema#double"
    >0.4</logic:low>
  </logic:FiftyFifty>
  <logic:Perhaps rdf:ID="perhaps">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >always use this singleton instance</rdfs:comment>
    <logic:high rdf:datatype="http://www.w3.org/2001/XMLSchema#double"
    >0.6</logic:high>
    <logic:low rdf:datatype="http://www.w3.org/2001/XMLSchema#double"
    >0.5</logic:low>
  </logic:Perhaps>
  <logic:Unlikely rdf:ID="unlikely">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >always use this singleton instance</rdfs:comment>
    <logic:high rdf:datatype="http://www.w3.org/2001/XMLSchema#double"
    >0.3</logic:high>
    <logic:low rdf:datatype="http://www.w3.org/2001/XMLSchema#double"
    >0.0</logic:low>
  </logic:Unlikely>
  <logic:Error rdf:ID="error">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >always use this singleton instance; the confidence interval for the (singleton) error instance is defined to be empty, thus no value needs to be assigned to high and low (but we might also use a NaN-like value to guarantee that the properties are always bound)</rdfs:comment>
  </logic:Error>
  <logic:Excluded rdf:ID="excluded">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >always use this singleton instance</rdfs:comment>
    <logic:low rdf:datatype="http://www.w3.org/2001/XMLSchema#double"
    >0.0</logic:low>
    <logic:high rdf:datatype="http://www.w3.org/2001/XMLSchema#double"
    >0.1</logic:high>
  </logic:Excluded>
  <logic:Confirmed rdf:ID="confirmed">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >always use this singleton instance</rdfs:comment>
    <logic:low rdf:datatype="http://www.w3.org/2001/XMLSchema#double"
    >0.9</logic:low>
    <logic:high rdf:datatype="http://www.w3.org/2001/XMLSchema#double"
    >1.0</logic:high>
  </logic:Confirmed>
  <logic:DontKnow rdf:ID="dontknow">
    <logic:high rdf:datatype="http://www.w3.org/2001/XMLSchema#double"
    >1.0</logic:high>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >always use this singleton instance</rdfs:comment>
    <logic:low rdf:datatype="http://www.w3.org/2001/XMLSchema#double"
    >0.0</logic:low>
  </logic:DontKnow>
  <logic:NotExcluded rdf:ID="notexcluded">
    <logic:low rdf:datatype="http://www.w3.org/2001/XMLSchema#double"
    >0.1</logic:low>
    <logic:high rdf:datatype="http://www.w3.org/2001/XMLSchema#double"
    >1.0</logic:high>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >always use this singleton instance</rdfs:comment>
  </logic:NotExcluded>
</rdf:RDF>

