assessQuizSession[quiz, responder]:

if (responder) {
  // Get the amount of questions answered, and the number of correct answers
  // returns total and correct as array.
  int[] results = quiz.getAssessment(I_ROBOT, responder);

  emitDA(#Inform(AssessingPerformance, theme=Answer,
                 agent=responder, total=^Integer.toString(results[0]),
                 correct=^Integer.toString(results[1])));
}
quiz.status = wait_for_ending;
